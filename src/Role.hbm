<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="com.ingesup.evaluationArena.hibernate.beans">
	<class name="Role" table="Role">
		<id
			column="ID"
			name="Id"
			type="integer"
		>
			<generator class="vm" />
		</id>
		<property
			column="Libelle"
			length="50"
			name="Libelle"
			not-null="true"
			type="string"
		 />
		<set inverse="true" name="UtilisateurRoleSet">
			<key column="Role_ID" />
			<one-to-many class="UtilisateurRole" />
		</set>
	</class>
</hibernate-mapping>
